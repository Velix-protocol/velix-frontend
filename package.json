{
  "name": "velix-frontend",
  "version": "1.27.0",
  "private": true,
  "scripts": {
    "start": "vite dist/landingPage --host 0.0.0.0",
    "start:app": "vite dist/app --host 0.0.0.0",
    "dev": "APP_MODE=landingPage vite",
    "dev:app": "APP_MODE=app vite",
    "build:landingPage:dev": "tsc && APP_MODE=landingPage vite build --mode development",
    "build:landingPage:stage": "tsc && APP_MODE=landingPage vite build --mode staging",
    "build:landingPage:prod": "tsc && APP_MODE=landingPage vite build --mode production",
    "build:app:prod": "tsc && APP_MODE=app vite build --mode production",
    "build:app:stage": "tsc && APP_MODE=app vite build --mode staging",
    "build:app:dev": "tsc && APP_MODE=app vite build --mode development",
    "build:dev": "pnpm run build:landingPage:dev && pnpm run build:app:dev",
    "build:stage": "pnpm run build:landingPage:stage && pnpm run build:app:stage",
    "build:prod": "pnpm run build:landingPage:prod && pnpm run build:app:prod",
    "build": "pnpm run build:landingPage:prod && pnpm run build:app:prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "release": "semantic-release"
  },
