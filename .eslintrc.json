{
  "extends": "next/core-web-vitals",
  "rules": {
    "import/no-extraneous-dependencies": ["warn", { "devDependencies": true }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "./src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }

}
